    "serverless-plugin-typescript": "^2.1.4",
    "serverless-prune-plugin": "2.0.1",
    "serverless-s3-local": "^0.7.1",
    "sonarqube-scanner": "^2.8.1",
    "sonarqube-verify": "^1.0.2",
    "ts-jest": "29.0.3",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.306.0",
    "@aws-sdk/s3-request-presigner": "^3.321.1",
    "@floteam/errors": "^1.0.8",
    "@redtea/format-axios-error": "^2.0.2",
    "@types/node": "16.18.2",
    "@types/validator": "^13.7.0",
    "aws-sdk": "^2.1048.0",
    "axios": "^0.24.0",
    "dotenv": "16.0.0",
    "dynamoose": "^2.8.3",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.7.2",
    "parse-multipart": "^1.0.4",
    "pexels": "^1.4.0",
    "piexifjs": "^1.0.6",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.12.2",
    "sequelize-typescript": "^2.1.1",
    "source-map-support": "^0.5.21",
    "uuid": "^8.3.2",
    "xml2js": "^0.4.23"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
